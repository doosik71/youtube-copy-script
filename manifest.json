{
  "manifest_version": 3,
  "name": "Youtube Copy Script",
  "description": "Copy script in Youtube",
  "version": "0.0.1",
  "action": {
    "default_popup": "main.html",
    "default_icon": {
      "16": "images/icon.png"
    }
  },
  "permissions": [
    "activeTab"
  ],
  "content_scripts": [
    {
      "matches": [
        "https://www.youtube.com/*"
      ],
      "js": [
        "scripts/content.js"
      ]
    }
  ]
}